{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "first_name": {
      "type": "string",
      "required": false
    },
    "last_name": {
      "type": "string",
      "required": false
    },
    "mobile_number": {
      "type": "biginteger",
      "required": false
    },
    "age": {
      "type": "integer"
    },
    "dob": {
      "type": "date"
    },
    "privacy_accepted": {
      "type": "boolean"
    },
    "getstarted_completed": {
      "type": "boolean"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "MALE",
        "FEMALE"
      ]
    },
    "user_experience_level": {
      "type": "enumeration",
      "enum": [
        "SCHOOL",
        "COLLEGE",
        "PROFESSIONAL"
      ]
    },
    "user_role": {
      "type": "enumeration",
      "enum": [
        "STUDENT",
        "TEACHER",
        "PARENT",
        "MARKETING",
        "ADMIN",
        "SUPERADMIN"
      ]
    },
    "profile_picture": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "org": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::org.org",
      "inversedBy": "users"
    },
    "user_status": {
      "type": "enumeration",
      "enum": [
        "PENDING",
        "APPROVED",
        "REJECTED",
        "BLOCKED"
      ]
    },
    "calander_activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::calander-activity.calander-activity",
      "mappedBy": "creator"
    },
    "team": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::team.team",
      "inversedBy": "players"
    },
    "home_address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::address.address",
      "inversedBy": "user_address"
    },
    "assigned_roles": {
      "type": "json"
    },
    "usersubscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::usersubscription.usersubscription",
      "mappedBy": "user"
    },
    "published_courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course",
      "mappedBy": "publisher"
    },
    "published_subjects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subject.subject",
      "mappedBy": "publishers"
    },
    "approved_subjects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subject.subject",
      "mappedBy": "approver"
    },
    "authored_subjects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subject.subject",
      "mappedBy": "authoredBy"
    },
    "grade": {
      "type": "enumeration",
      "enum": [
        "PLAYSCHOOL",
        "LKG",
        "UKG",
        "FIRST",
        "SECOND",
        "THIRD",
        "FOURTH",
        "FIFTH",
        "SIXTH",
        "SEVENTH",
        "EIGHTH",
        "NINTH",
        "TENTH",
        "ELEVENTH",
        "TWELFTH",
        "DIPLOMA",
        "GRADUATION",
        "POSTGRADUATION",
        "PHD"
      ]
    },
    "user_profiles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-profile.user-profile",
      "mappedBy": "user"
    },
    "published_courses_pricing": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-pricing.course-pricing",
      "mappedBy": "publisher"
    },
    "subjects_published_pricing": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subject-pricing.subject-pricing",
      "mappedBy": "publisher"
    },
    "created_pricing": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pricing-offer.pricing-offer",
      "mappedBy": "creator"
    },
    "created_offers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pricing-offer.pricing-offer",
      "mappedBy": "target_publisher"
    },
    "created_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::invoice-item.invoice-item",
      "mappedBy": "publisher"
    }
  }
}
