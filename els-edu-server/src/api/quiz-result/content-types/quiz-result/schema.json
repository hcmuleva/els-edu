{
  "kind": "collectionType",
  "collectionName": "quiz_results",
  "info": {
    "singularName": "quiz-result",
    "pluralName": "quiz-results",
    "displayName": "Quiz Result",
    "description": "Stores user quiz attempts and scores"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "score": {
      "type": "integer",
      "required": true
    },
    "totalQuestions": {
      "type": "integer",
      "required": true
    },
    "percentage": {
      "type": "integer"
    },
    "answers": {
      "type": "json"
    },
    "timeTaken": {
      "type": "integer"
    },
    "questionTimings": {
      "type": "json"
    },
    "startedAt": {
      "type": "datetime"
    },
    "completedAt": {
      "type": "datetime"
    },
    "attemptNumber": {
      "type": "integer",
      "default": 1
    },
    "isPassed": {
      "type": "boolean"
    },
    "correctAnswers": {
      "type": "integer"
    },
    "incorrectAnswers": {
      "type": "integer"
    },
    "unansweredQuestions": {
      "type": "integer",
      "default": 0
    },
    "questionAnalysis": {
      "type": "json"
    },
    "weakAreas": {
      "type": "json"
    },
    "strongAreas": {
      "type": "json"
    },
    "recommendedRetake": {
      "type": "boolean",
      "default": false
    },
    "retakeQuestions": {
      "type": "json"
    },
    "improvementSuggestions": {
      "type": "json"
    },
    "deviceInfo": {
      "type": "json"
    },
    "sessionData": {
      "type": "json"
    },
    "topic": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::topic.topic",
      "inversedBy": "quizResults"
    },
    "subject": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subject.subject",
      "inversedBy": "quizResults"
    },
    "questions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::question.question"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
