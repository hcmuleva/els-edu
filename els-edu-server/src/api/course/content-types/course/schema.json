{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "publisher": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "published_courses"
    },
    "creator": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "organization": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::org.org",
      "inversedBy": "courses"
    },
    "subjects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subject.subject",
      "mappedBy": "courses"
    },
    "usersubscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::usersubscription.usersubscription",
      "mappedBy": "course"
    },
    "tags": {
      "type": "json"
    },
    "category": {
      "type": "string"
    },
    "subcategory": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "condition": {
      "type": "enumeration",
      "enum": [
        "DRAFT",
        "REVIEW",
        "REJECT",
        "APPROVED",
        "PUBLISH",
        "RETIRED"
      ]
    },
    "privacy": {
      "type": "enumeration",
      "enum": [
        "PUBLIC",
        "PRIVATE",
        "ORG",
        "OPEN"
      ]
    },
    "visibility": {
      "type": "enumeration",
      "enum": [
        "GLOBAL",
        "ORG",
        "OTHER"
      ]
    },
    "coverphoto": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "previewvideo": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
