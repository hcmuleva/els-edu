{
  "kind": "collectionType",
  "collectionName": "contents",
  "info": {
    "singularName": "content",
    "pluralName": "contents",
    "displayName": "content"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "YOUTUBE",
        "VIDEO",
        "IMAGE",
        "MD",
        "TEXT",
        "DOCUMENT",
        "DOWNLOAD"
      ]
    },
    "json_description": {
      "type": "blocks"
    },
    "multimedia": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "youtubeurl": {
      "type": "string"
    },
    "subjects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subject.subject",
      "inversedBy": "contents"
    },
    "topics": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::topic.topic",
      "inversedBy": "contents"
    },
    "quizzes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::quiz.quiz",
      "mappedBy": "contents"
    },
    "creator": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "resources": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::resource.resource",
      "mappedBy": "content"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "TIPSTRICS",
        "FORMULA",
        "CONTENT",
        "READING",
        "OTHER"
      ]
    },
    "thumbnail": {
      "type": "string"
    }
  }
}
