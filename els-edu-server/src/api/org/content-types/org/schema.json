{
  "kind": "collectionType",
  "collectionName": "orgs",
  "info": {
    "singularName": "org",
    "pluralName": "orgs",
    "displayName": "org"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "org_name": {
      "type": "string"
    },
    "contact_email": {
      "type": "email"
    },
    "contact_phone": {
      "type": "biginteger"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "org_status": {
      "type": "enumeration",
      "default": "ACTIVE",
      "enum": [
        "ACTIVE",
        "INACTIVE"
      ]
    },
    "description": {
      "type": "blocks"
    },
    "users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "org"
    },
    "courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course.course",
      "mappedBy": "organizations"
    },
    "pricings": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::pricing.pricing",
      "mappedBy": "orgs"
    },
    "address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::address.address",
      "mappedBy": "org"
    },
    "calander_activities": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::calander-activity.calander-activity",
      "mappedBy": "orgs"
    },
    "teams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::team.team",
      "mappedBy": "org"
    },
    "usersubscriptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::usersubscription.usersubscription",
      "mappedBy": "org"
    }
  }
}
