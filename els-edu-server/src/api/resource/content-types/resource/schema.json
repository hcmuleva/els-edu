{
  "kind": "collectionType",
  "collectionName": "resources",
  "info": {
    "singularName": "resource",
    "pluralName": "resources",
    "displayName": "resource"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "resource_url": {
      "type": "string"
    },
    "resource_packages": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "resource_type": {
      "type": "enumeration",
      "enum": [
        "PDF",
        "VIDEO",
        "CODE",
        "FILE"
      ]
    },
    "content": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::content.content",
      "inversedBy": "resources"
    }
  }
}
